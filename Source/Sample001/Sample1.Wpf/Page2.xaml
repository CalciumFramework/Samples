<Page x:Class="Sample1.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sample1"
      xmlns:sample1="clr-namespace:Sample1;assembly=Sample1"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Page 2" 
	  d:DataContext="{d:DesignInstance sample1:Page2ViewModel}">

	<StackPanel Style="{StaticResource RootPanel}">
		<TextBlock>Page 2</TextBlock>
		<Button Command="{Binding NavigateBackCommand}" Content="Back" />
	</StackPanel>
	
</Page>
